snippet fac "fac"
Factory(:${1}, ${2})${3}

endsnippet

snippet facb "facb"
Factory.build(:${1}, ${2})${3}

endsnippet

snippet facd "facd"
Factory.define(:${1:model}) do |${2:f}|
	${3}
end
${4}

endsnippet

snippet facn "facn"
Factory.next(:${1:sequence-name})${2}

endsnippet

snippet facs "facs"
Factory.sequence :${1:sequence-name} do |${2:m}|
	${3}
end
${4}

endsnippet

